# Default make file
PKG_CPPFLAGS=-I.
PKG_LIBS=$(ARCHLIB) $(LAPACK_LIBS) $(BLAS_LIBS) $(FLIBS)

# Retrieve all cpp files in src/A and src/B directories
SOURCES=r-accessor-to-code.cpp @SUBDIR_SOURCES@

# Obtain the object files
OBJECTS=$(SOURCES:.cpp=.o)

all: $(SHLIB)

# Provide recipe to remove all objects
clean:
	@rm -f $(OBJECTS)
